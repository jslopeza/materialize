header
  if no_nav == false
    nav
      .container
        .nav-wrapper
          a.page-title #{page}
  .container
    a.button-collapse(href='#', data-activates='nav-mobile', class=(no_nav == false ? "full" : ""))
      i.mdi-navigation-menu
  ul#nav-mobile.side-nav.fixed
    li(class="logo")
      a#logo-container.brand-logo(href='http://materializecss.com/')
        object#front-page-logo(type='image/svg+xml', data='res/materialize.svg') Your browser does not support SVG
    li(class=(page == "About" ? "active" : ""))
      a(href='http://materializecss.com/about.html') About
    li(class=(page == "Getting Started" ? "active" : ""))
      a(href='http://materializecss.com/getting-started.html') Getting Started

    li.no-padding
      ul.collapsible.collapsible-accordion
        li
        a.collapsible-header.active
          | CSS
        .collapsible-body
          ul
            li(class=(page == "Sass" ? "active" : ""))
              a(href='sass.html') Sass
            li(class=(page == "Typography" ? "active" : ""))
              a(href='typography.html') Typography
            li(class=(page == "Color" ? "active" : ""))
              a(href='typography.html') Color
            li
              a(href='#!') Fourth Dropdown Link

    li(class=(page == "Components" ? "active" : ""))
      a(href='http://materializecss.com/components.html') Components
    li(class=(page == "JavaScript" ? "active" : ""))
      a(href='http://materializecss.com/javascript.html') JavaScript
    li(class=(page == "Mobile" ? "active" : ""))
      a(href='http://materializecss.com/mobile.html') Mobile